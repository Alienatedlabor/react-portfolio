import React from 'react';

export default function Project({
  title,
  description,
  image,
  alt,
}: {
  title: string;
  description: string;
  image: string;
  alt: string;
}) {
  return (
    <div>
      <h1>{title}</h1>
      <img src={image} alt={alt}></img>
      <p>{description}</p>
    </div>
  );
}
